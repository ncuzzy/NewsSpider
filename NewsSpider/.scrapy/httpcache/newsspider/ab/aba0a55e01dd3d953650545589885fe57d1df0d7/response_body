<script>

    defaultImg()
    lazyloadImg();

    function lazyloadImg() {
        $("#lazyload img").bind("scrollin", function () {
            var img = this,
                    $img = $(img);
            $img.unbind("scrollin");
            if ($img.attr("img_src")) {
                img.src = $img.attr("img_src");
            }
        });
    }
    function defaultImg() {

        $("#lazyload img").each(function (index) {
            var img = this, $img = $(img);
            var imgUrl = "http://static.qcdqcdn.com/img/qcdq.png";
            var wh = $img.attr("data");
            if(wh){
                var width = parseInt(wh.substring(wh.indexOf("width:") + 6,wh.indexOf(";h")));
                var heigth = parseInt(wh.substring(wh.indexOf("height:") + 7,wh.lastIndexOf(";")));
                if(width && heigth && heigth > 0 && width + 200  >heigth  ){
                    imgUrl = "http://img2.qcdqcdn.com/group2/M00/1D/36/pIYBAFncjiyAJpNiAAAF5inzF3Y709.png";
                    imgUrl = imgUrl.replace(".png","_" + width + "x" + heigth +".png")
                }else if(width && heigth && width > 0 && width  < heigth){
                    imgUrl = "http://img2.qcdqcdn.com/group2/M00/1D/36/pIYBAFncjlGASsQIAAAJmXv7NSc754.png";
                    imgUrl = imgUrl.replace(".png","_" + width + "x" + heigth +".png")
                }
            }
            if ($img.attr("img_src")) {
                img.src = imgUrl;
            }else{
                $img.attr("img_src",img.src);
                img.src = imgUrl;
            }
        });
    }


    $(".textarea").placeholder({isUseSpan:true});
    $(".reply_box").placeholder({isUseSpan:true});



    var clones=$('.yuedu');
    $(".tcdPageCode").createPage({
        pageCount: 1,
        current: 1,
        backFn: function (p) {
            var url = "/News/Preview/242634-"+p+".do";
            url = "http://news.qichedaquan.com/xincheshangshi/20180518/242634-"+p+".html";
            window.location.href = url;
            $('.tcdPageCode').append(clones);
        }
    });
    $(".read_all").on('click',function(){
        if($(".read_all").hasClass("showed"))return;
        var cur="1";
        var purl = "";
            purl = "http://news.qichedaquan.com/xincheshangshi/20180518/242634-all.html";
        if(cur == -1){
            window.location.href = purl;
        }else {
            var url = purl + '#p_' + (cur);
            window.location.href = url;
        }

    })

    $(".yuedu").on('click',function(){
        var cur="1";
        var purl = "";
            purl = "http://news.qichedaquan.com/xincheshangshi/20180518/242634-all.html";
            if(cur == -1){
                window.location.href = purl;
            }else {
                var url = purl + '#p_' + (++cur);
                window.location.href = url;
            }

        })




    $('.tcdPageCode').append(clones);

</script>
