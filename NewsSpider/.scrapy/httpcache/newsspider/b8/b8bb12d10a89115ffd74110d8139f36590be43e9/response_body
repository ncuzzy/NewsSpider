        <script type="text/javascript">
            var commentorderby = "new";
            GetNewsViewComments(1); ShowReply(0); GetRelatedNews(577568,'锂电池');

        var shouc = 0;
            if (shouc==1) {
                $("#a_shoucang").removeAttr("onclick").html("已收藏");
            }

            $.ajax({
                type: 'GET',
                url: '/m/apptuijian.ashx?Tid=1',
                // data to be added to query string:
                data: {},
                // type of data we are expecting in return:
                dataType: 'text',
                timeout: 10000,
                success: function (data) {

                    var mydata = eval("(" + data + ")");
                  
                    if (mydata.length > 0) {
                        var str="";
                        for (var i = 0; i < mydata.length; i++) {
                            var appname = mydata[i].APPName;
                            var appurl = mydata[i].APPUrl;
                            var imgurl = mydata[i].ImgUrl;

                            str += "<li><a href=\"" + appurl + "\"><img src=\"" + imgurl + "\" width=\"101\" height=\"100\"><p>" + appname + "</p></a></li>";
                        }

                        $("#apptuijian").append(str);
                    }
                },
                error: function (xhr, type) {
                    //alert('Ajax error!')
                }
            })
            $("#gotop").click(function () {
                window.scrollTo(0, 1);
                $("#gotop").hide();
            });
            $(window).scroll(function () {
                if ($(".share").offset().top < window.scrollY + window.innerHeight) {
                    $("#gotop").show();
                }
                if ($(this).scrollTop() > 0) {
                    //$("#gotop").show();
                }
                else { $("#gotop").hide(); }
            });
        
            var mc = getCookie("mydrivers_userid");
            if (mc != null && mc != "") {
                //加载评论提醒
                $.ajax({
                    type: 'POST',
                    url: '/m/mycommentsnums.ashx',
                    // data to be added to query string:
                    data: { act: "getcommentsreplynums" },
                    // type of data we are expecting in return:
                    dataType: 'text',
                    timeout: 10000,
                    success: function (data) {
                        if (parseInt(data) > 0) {
                            $(".eloginbar").append("<div class=\"hcircle\"></div>");
                        }
                    },
                    error: function (xhr, type) {
                        //alert('Ajax error!')
                    }
                });
            }         

    </script>
